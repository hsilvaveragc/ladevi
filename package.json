{
  "name": "ladevi-ventas-front",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:staging": "vite --mode staging",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode production",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "preview:test": "vite preview --mode test",
    "preview:prod": "vite preview --mode production",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:setup": "docker-compose -f testing/docker-compose.test.yml up --build -d",
    "test:e2e:teardown": "docker-compose -f testing/docker-compose.test.yml down -v",
    "test:e2e:full": "bash testing/run-tests.sh",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 20",
    "lint:strict": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,json,css,md}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,json,css,md}'",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.6",
    "axios": "1.11.0",
    "bootstrap": "4.3.1",
    "cheeseburger-menu": "0.0.13",
    "classnames": "2.5.1",
    "connected-react-router": "6.9.3",
    "date-fns": "2.30.0",
    "formik": "2.4.6",
    "history": "4.9.0",
    "jspdf": "2.5.2",
    "jspdf-autotable": "3.8.3",
    "query-string": "8.2.0",
    "ramda": "0.26.1",
    "react": "18.3.1",
    "react-bootstrap-table-next": "4.0.3",
    "react-bootstrap-table2-paginator": "2.1.2",
    "react-bootstrap-table2-toolkit": "2.1.2",
    "react-csv": "2.0.1",
    "react-datepicker": "4.25.0",
    "react-dom": "18.3.1",
    "react-hamburger-menu": "1.1.1",
    "react-input-mask": "^2.0.4",
    "react-loading-skeleton": "^3.5.0",
    "react-modal": "3.16.3",
    "react-number-format": "5.4.4",
    "react-redux": "8.1.3",
    "react-router": "5.0.1",
    "react-router-breadcrumbs-hoc": "3.2.0",
    "react-router-dom": "5.0.1",
    "react-select": "5.10.2",
    "react-table": "6.10.0",
    "react-toastify": "9.1.3",
    "reactstrap": "8.0.0",
    "redux": "4.2.1",
    "redux-logger": "3.0.6",
    "redux-saga": "1.0.3",
    "reselect": "4.1.8",
    "styled-components": "6.1.19",
    "xlsx": "0.18.5",
    "yup": "1.7.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@babel/preset-react": "^7.27.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.1.0",
    "body-parser": "1.19.0",
    "commitizen": "3.1.1",
    "connect-pause": "0.1.0",
    "cors": "2.8.5",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "husky": "^9.1.7",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "8.2.0",
    "morgan": "1.9.1",
    "nodemon": "1.19.1",
    "prettier": "3.6.2",
    "prop-types": "15.7.2",
    "vite": "^5.4.20",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^0.34.6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "src/*"
    ]
  },
  "volta": {
    "node": "18.20.8"
  }
}
