{
  "name": "ladevi-ventas-front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "eject": "react-scripts eject",
    "format": "prettier --write '{src,__{tests,mocks}__}/**/*.{js,jsx,json,scss}'",
    "start": "NODE_OPTIONS=--openssl-legacy-provider npm-run-all -p start:js",
    "start:js": "react-scripts start",
    "build": "NODE_OPTIONS=--openssl-legacy-provider react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:setup": "docker-compose -f testing/docker-compose.test.yml up --build -d",
    "test:e2e:teardown": "docker-compose -f testing/docker-compose.test.yml down -v",
    "test:e2e:full": "bash testing/run-tests.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "axios": "0.19.0",
    "bootstrap": "4.3.1",
    "cheeseburger-menu": "0.0.13",
    "classnames": "2.2.6",
    "connected-react-router": "6.4.0",
    "date-fns": "1.30.1",
    "formik": "1.5.7",
    "history": "4.9.0",
    "jspdf": "2.5.2",
    "jspdf-autotable": "3.8.3",
    "moment": "2.24.0",
    "query-string": "6.8.2",
    "ramda": "0.26.1",
    "react": "16.8.6",
    "react-bootstrap-table-next": "3.3.5",
    "react-bootstrap-table2-paginator": "2.1.0",
    "react-bootstrap-table2-toolkit": "2.1.2",
    "react-csv": "2.0.1",
    "react-data-export": "0.6.0",
    "react-datepicker": "2.8.0",
    "react-dom": "16.8.6",
    "react-hamburger-menu": "1.1.1",
    "react-input-mask": "^2.0.4",
    "react-loading-skeleton": "^3.5.0",
    "react-modal": "3.8.1",
    "react-number-format": "4.4.1",
    "react-redux": "7.2.9",
    "react-router": "5.0.1",
    "react-router-breadcrumbs-hoc": "3.2.0",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.4.1",
    "react-select": "3.0.4",
    "react-table": "6.10.0",
    "react-toastify": "6.0.0",
    "reactstrap": "8.0.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "1.0.3",
    "reselect": "4.0.0",
    "styled-components": "4.3.1",
    "xlsx": "0.15.6",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "body-parser": "1.19.0",
    "commitizen": "3.1.1",
    "connect-pause": "0.1.0",
    "cors": "2.8.5",
    "cz-conventional-changelog": "2.1.0",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "husky": "2.4.0",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "8.2.0",
    "morgan": "1.9.1",
    "nodemon": "1.19.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "prop-types": "15.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,json,scss,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "src/*"
    ]
  },
  "volta": {
    "node": "18.20.8"
  }
}
